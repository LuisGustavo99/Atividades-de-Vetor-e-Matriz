programa
{
	
	funcao inicio()
	{
		inteiro numeros
		inteiro lista[10]
			para(inteiro i=0; i <10; i++){
				escreva("informe 10 valores")
				leia(numeros)
				lista[i] = numeros
			}
			para(inteiro i=0; i <10; i++){
			  	se(lista[i] % 2 == 0)
			  	{
			  		escreva("\n3O numero ", lista[i] , " Ã© 1pares")
			  	}
			}
	}
}
